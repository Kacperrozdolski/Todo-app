{
    "name": "To do app",
    "description": "To do app",
    "version": "0.1.0",
    "manifest_version": 2,
    "icons":{
        "16":"./16x16.png",
        "32":"./32x32.png",
        "48":"./48x48.png",
        "128":"./128x128.png"
    },
    
    "background": {
        "scripts": ["./popup.js"]
    },
    "web_accessible_resources": [
        "style.css",
        "font-awsome.css",
        "https://fonts.googleapis.com/css?family=Titillium+Web"
    ],

    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions":[
        "tabs",
        "https://www.google.com/*",
        "storage",
        "cookies"
    ]
}